<!DOCTYPE html>
<html>
<head><title>google_form_liff</title></head>
 
<body>
 
<script src="https://static.line-scdn.net/liff/edge/2.1/liff.js"></script>
 
<script>
 
  async function getUserProfile() {
   const profile = await liff.getProfile()
   window.location.replace(
         `https://docs.google.com/forms/d/e/1FAIpQLScCXaUMfIQ1RKl5IW0jCYcEZSWIsTGi8gGFqkzX2MJ2L6_sSQ/viewform?usp=pp_url
         &entry.1223295606=${profile.userId}
         &entry.2100067262=${profile.displayName}
         &entry.705853642=${profile.pictureUrl}`
     );
   }
 
  async function main() {
     await liff.init({ liffId: "1654176509-x1bqoMw0" })
       if (liff.isLoggedIn()) {
         getUserProfile()
       } else {
         liff.login()
       }
   }
   main()
 </script>
 
 </body>
</html>
